
export enum Interpolation
    none = 0
    smooth = 1
    linear = 2

export default class KeyFrame<T>

    time: Number
    value: Any
    interpolation: Interpolation = Interpolation.smooth

    constructor()
        time: Number
        value: Any
        interpolation: Interpolation = Interpolation.smooth
        ->
        this.time = time
        this.value = value
        this.interpolation = interpolation
